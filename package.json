{
  "name": "zombieplus",
  "version": "1.0.0",
  "description": "automated testing project with nightwatch",
  "main": "index.js",
  "scripts": {
    "test:ci": "./node_modules/.bin/nightwatch --env=headless",
    "test:ciculqi": "./node_modules/.bin/nightwatch --tag culqi --env=headless",
    "test:cidemo": "./node_modules/.bin/nightwatch --tag demo --env=headless",
    "test:admin": "./node_modules/.bin/nightwatch --tag admin --env=stageadmin"
  },
  "keywords": [
    "nodejs",
    "webdriver",
    "nightwatch",
    "chromedriver",
    "geckodriver"
  ],
  "author": "Ivan Mora",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-preset-es2015": "^6.24.1",
    "chromedriver": "^80.0.0",
    "faker": "^4.1.0",
    "geckodriver": "^1.19.1",
    "nightwatch": "^1.2.4",
    "pg": "^7.13.0"
  },
  "dependencies": {}
}
